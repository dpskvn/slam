  <div>
    <h2 id="x">X</h2>
    <h2 id="y">Y</h2>
    <h2 id="z">Z</h2>
  </div>
  <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script async src="/js/mobile.js"></script>
  <% } else { %>
    <h2>Wrong game ID</h2>
  <% } %>
</body>
</html>